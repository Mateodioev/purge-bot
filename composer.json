{
    "name": "mateodioev/purge-bot",
    "description": "Bot for delete messages in telegram",
    "type": "project",
    "require": {
        "mateodioev/tg-handler": "^5.1",
        "vlucas/phpdotenv": "^5.5",
        "amphp/http-server": "^3.2",
        "monolog/monolog": "^3.4",
        "amphp/log": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.35"
    },
    "scripts": {
        "fix": "php-cs-fixer fix --config=.php-cs-fixer.dist.php --verbose"
    }
}
